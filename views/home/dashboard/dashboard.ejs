<div class="col-12">
    <%- include(`../../partials/notification`) %>
    <div class="card flex-fill">
        <div class="card-header pb-0 d-flex justify-content-between align-items-center">
            <h4 class="card-title mb-0">Active Projects</h4>
            <div class="dropdown">
                <a href="javascript:void(0);" data-bs-toggle="dropdown" aria-expanded="false" class="dropset">
                    <i class="fa fa-ellipsis-v"></i>
                </a>
                <ul class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                    <li>
                        <a href="" class="dropdown-item">Licences List</a>
                    </li>
                    <li>
                        <a href="/dashboard/newApplication" class="dropdown-item">Add Engineering Project</a>
                    </li>
                </ul>
            </div>
        </div>
        <div class="card-body">
            <div class="table-responsive">
                <table class="table datatable">
                    <thead>
                        <tr>
                            <th>Project Title</th>
                            <th>Type</th>
                            <th>Project Value (USD)</th>
                            <th>Duration (Days)</th>
                            <th>Status</th>
                            <th>Objectives</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% activeProjects.forEach((project, index) => { %>
                        <tr>
                            <td><a href="#" class="text-dark"><%= project.title %></a></td>
                            <td><span ><%= project.type %></span></td>
                            <td>$<%= project.projectValue.toLocaleString() %></td>
                            <td><%= project.duration %></td>
                            <td>
                                <span class="badge <%= project.status.toLowerCase() === 'active' ? 'bg-success' : 'bg-warning' %>">
                                    <%= project.status %>
                                </span>
                            </td>
                            <td><%= project.objectives %></td>
                        </tr>
                        <% }) %>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>

