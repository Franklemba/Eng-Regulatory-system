<div class="page-wrapper">
    <div class="content">
      <div class="page-header">
        <div class="page-title">
          <h4>Submitted Licenses</h4>
          <h6>Overview of all licenses and certifications submitted</h6>
        </div>
      </div>
  
      <div class="card">
        <div class="card-body">
          <!-- Check if there are submitted licenses -->
          <% if (submittedLicenses && submittedLicenses.length > 0) { %>
            <div class="table-responsive">
              <table class="table table-bordered">
                <thead class="thead-dark">
                  <tr>
                    <th>#</th>
                    <th>Company Name</th>
                    <th>Membership Class</th>
                    <th>Country</th>
                    <th>Additional Info</th>
                    <th>Submission Date</th>
                  </tr>
                </thead>
                <tbody>
                  <% submittedLicenses.forEach((license, index) => { %>
                    <tr>
                      <td><%= index + 1 %></td>
                      <td><%= license.companyName %></td>
                      <td><%= license.membershipClass %></td>
                      <td><%= license.country || "No" %></td>
                      <td><%= license.additionalInfo || "N/A" %></td>
                      <td><%= new Date(license.createdAt).toLocaleDateString() %></td>
                    </tr>
                  <% }) %>
                </tbody>
              </table>
            </div>
          <% } else { %>
            <p class="text-center">No licenses submitted yet. Please add some.</p>
          <% } %>
        </div>
      </div>
    </div>
  </div>
  